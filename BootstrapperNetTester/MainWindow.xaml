<Window x:Class="BootstrapperNetTester.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BootstrapperNetTester"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel HorizontalAlignment="Center"
                    VerticalAlignment="Center">
            <Button Content="Test transient service" Command="{Binding TestTransientServiceCommand}" />
            <Button Content="Test singleton service" Command="{Binding TestSingletonServiceCommand}"/>
        </StackPanel>
    </Grid>
</Window>
